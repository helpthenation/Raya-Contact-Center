<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

  <!-- new -->
  <t t-name="hr_check_button">
      <td class="pr-3">
          <button name="show" arial-label="warning" class="btn btn-warning" type="button">
              <i class="fa fa-refresh"/>
              <!-- fa-repeat -->
          </button>
      </td>
  </t>


  <t t-name="hr_skill_data_row">
      <tr class="o_data_row" t-att-data-id="id">
          <td class="o_data_cell o_skill_cell w-100">
              <t t-esc="data.skill_id.data.display_name"/>
          </td>
          <td class="o_data_cell o_skill_cell pr-1">
              <t t-esc="data.current_degree"/>%
          </td>
          <td class="o_data_cell o_skill_cell pr-3">
              <t t-esc="data.skill_level_id.data.display_name"/>
          </td>

          <td class="o_data_cell o_skill_cell pr-1">
              <t t-esc="data.validation_date._i"/>
          </td>


      </tr>
  </t>

</templates>
