<odoo>
    <template id="portal_checklist_layout" name="Portal Check List Layout">
        <t t-call="portal.frontend_layout">
            <t t-set="is_portal" t-value="True"/>

            <div t-if="not no_breadcrumbs and not my_details and not breadcrumbs_searchbar" class="o_portal container mt-3">
                <div class="row align-items-center bg-white no-gutters border rounded">
                    <div class="col-10">
                        <t t-call="ow_portal.portal_breadcrumbs_checkList"></t>
                    </div>
                    <div t-if="prev_record or next_record" class="col-2 flex-grow-0 text-center">
                        <t t-call='portal.record_pager'/>
                    </div>
                </div>
            </div>
            <div id="wrap" class='o_portal_wrap'>
                <div class="container mb64">
                    <t t-if="my_details">
                        <div class="row justify-content-between mt-4">
                            <div t-attf-class="col-12 col-md col-lg-6">
                                <t t-raw="0"/>
                            </div>
                            <div id="o_my_sidebar" class="pt-3 pt-lg-0 col-12 col-md col-lg-4 col-xl-3 o_my_sidebar">
                                <div class="o_my_contact" t-if="sales_user">
                                    <t t-call="portal.portal_contact"/>
                                </div>
                                <div class="o_portal_my_details">
                                    <h4>Details <a role="button" href="/my/account" class="btn btn-sm btn-link"><i class="fa fa-pencil"/> Edit</a></h4>
                                    <hr class="mt-1 mb-0"/>
                                    <div t-field="user_id.partner_id" t-options='{"widget": "contact", "fields": ["email", "phone", "address", "name"]}'/>
                                </div>
                                <div class="o_portal_my_security mt-3">
                                    <h4>Account Security </h4>
                                    <hr class="mt-1 mb-1"/>
                                    <a href="/my/security"><i class="fa fa-pencil mx-1"/>Edit Security Settings</a>
                                </div>
                            </div>
                        </div>
                    </t>
                    <t t-else="">
                        <t t-raw="0"/>
                    </t>
                </div>
            </div>
        </t>
    </template>
    <template id="portal_breadcrumbs_checkList" name="Portal Breadcrumbs">
        <ol t-if="page_name != 'home'" class="o_portal_submenu breadcrumb mb-0 py-2 flex-grow-1 row">
            <li class="breadcrumb-item ml-1"><a href="/my/home" aria-label="Home" title="Home"><i class="fa fa-home"/></a></li>
            <li t-if="page_name == 'my_details'" class="breadcrumb-item">Details</li>
        </ol>
    </template>
<!-- ########################### LayOut ##################################-->
<template name="My Check List" t-name="ow_portal.portal_check_list" id="portal_check_list" >
        <t t-call="ow_portal.portal_checklist_layout">
            <t t-set="my_details" t-value="True"/>
            <div class="o_portal_my_home">
                <div class="oe_structure" id="oe_structure_portal_my_home_1"/>
                <h3>Check List</h3>
                <div class="o_portal_docs list-group">
                </div>
            </div>
            <div class="oe_structure" id="oe_structure_portal_my_home_2"/>
        </t>
    </template>

    <template id="portal_width" inherit_id="ow_portal.portal_checklist_layout">
        <xpath expr="//div[hasclass('row')]/div[@t-attf-class='col-12 col-md col-lg-6']" position="attributes">
            <attribute name="t-attf-class">col-12 col-md-12 col-lg-9</attribute>
        </xpath>
        <xpath expr="//div[@id='o_my_sidebar']" position="attributes">
            <attribute name="class">col-lg-3 o_my_sidebar</attribute>
        </xpath>
    </template>
    
    <template id="portal_blocks" inherit_id="ow_portal.portal_check_list">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="attributes">
            <attribute name="class">o_portal_docs list-group ow_portal</attribute>
        </xpath>
    </template>
<!-- ############################################################################# -->

<template id="portal_my_home_check_list" name="Portal layout : Checklist menu entry" inherit_id="ow_portal.portal_breadcrumbs_checkList" priority="15">
        <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
            <!-- ######################### Non Technical Skills Box ###########################-->
            <li t-if="page_name == 'online' or online" t-attf-class="breadcrumb-item #{'active ' if not online else ''}">
                <a t-if="online" t-attf-href="/my/online?{{ keep_query() }}">
                    Online 
                </a>
                <t t-else="">
                    Online
                </t>
            </li>
            <!-- ############################ Technical Skills Box ###########################-->
            <li t-if="page_name == 'g6' or g6" t-attf-class="breadcrumb-item #{'active ' if not g6 else ''}">
                <a t-if="g6" t-attf-href="/my/g6?{{ keep_query() }}">
                    Grade 6
                </a>
                <t t-else="">
                    Grade 6
                </t>
            </li>
            <!-- ######################### Languages Skills Box  ############################-->
            <li t-if="page_name == 'entry' or entry" t-attf-class="breadcrumb-item #{'active ' if not entry else ''}">
                <a t-if="entry" t-attf-href="/my/entry?{{ keep_query() }}">
                    Entry Checklist
                </a>
                <t t-else="">
                    Entry Checklist
                </t>
            </li>
        </xpath>
    </template>
    <!-- ################################################################################ -->
    
    <!-- ############################## Home Landing Page Links ############################# -->
    <template id="portal_my_home_check_lists" name="Check Lists" customize_show="True" inherit_id="ow_portal.portal_check_list" priority="15">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <!-- ################  Non Technical Skills ##################### -->
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Online</t>
                <t t-set="url" t-value="'/my/online'"/>
              <!--  <t t-set="placeholder_count" t-value="'skills_count'"/> -->
            </t>
            <!-- ################  Technical Skills ################# -->
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Grade 6</t>
                <t t-set="url" t-value="'/my/g6'"/>
                <!--<t t-set="placeholder_count" t-value="'tec_skills_count'"/> -->
            </t>
            <!-- ################  Language Skills ################# -->
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Entry List</t>
                <t t-set="url" t-value="'/my/entry'"/>
                <!-- <t t-set="placeholder_count" t-value="'lang_skills_count'"/> -->
            </t>
        </xpath>
    </template>

<!-- ############################## Non Tech Skills Tree Page ######################## -->
    <template id="portal_my_online" name="Online Guide">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True"/>
            <t t-call="portal.portal_searchbar">
                <t t-set="title">Online Guide</t>
            </t>
			
            <div t-if="not onlines" class="alert alert-warning mt8" role="alert">
                There are no Online Guide for you.
            </div>
            <t t-if="onlines" t-call="portal.portal_table">
                <thead>
                    <tr>
                        <th class="w-75">Name</th>
                        <th >Type</th>
                        <th>View</th>
                        <th>URL</th>
                    </tr>
                </thead>
                <tbody>
                    <tr t-foreach="onlines" t-as="online">
                        <td>
                          <span t-field="online.name"/>
                        </td>
                        <td><span t-field="online.type" /></td>
                        <td>
                            <a t-att-href="'/web/content/%i?download=false' % online.attachment.id">
                                <span t-esc="online.attachment.name" class="fa fa-download" />
                            </a>
                        </td>
                        <td>
                            <iframe width="420" height="315" src="online.url"> </iframe>
                                
                        </td>
                      
                        
                    </tr>
                </tbody>
                
                <thead>
                    <tr>
                        <th class="w-75">Entry Name</th>
                        <th >Done</th>
                    </tr>
                </thead>
                <tbody>
                    <tr t-foreach="entrylist" t-as="ent">
                        <td>
                          <span t-field="ent.name"/>
                        </td>
                        <td><span t-field="ent.done" /></td>
                    </tr>
                </tbody>
            </t>
        </t>
    </template>
    
  
</odoo>